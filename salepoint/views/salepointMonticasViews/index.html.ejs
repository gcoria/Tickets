<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>.: Monticas :.</title>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.3/angular.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.3/angular-route.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
    <script src="/js/script.js"></script>
  </head>
  <body>
    <div>
      <nav class="navbar navbar-inverse">
        <div class="container-fluid">
          <div class="navbar-header">
            <a class="navbar-brand" href="/">Web Site Monticas</a>
          </div>
            <ul class="nav navbar-nav">
              <li class="active"><a href="/">Home</a></li>
              <li><a href="#">Promociones</a></li>
              <li><a href="#">Atencion al cliente</a></li>
            </ul>
        </div>
      </nav>
       
      <div class="jumbotron text-center">
        <div class="container">
          <div class="page-header">
            <h1>Elija su Paquete</h1>
          </div>

          <!-- origin city -->
          <div class="row">
            <div class="col-md-6 col-md-offset-1">
              <div class="form-group">
                <div class="col-md-6">
                   <h3>Ciudad de Origen</h3>
                  <select class="form-control" id="origin">
                    <option></option>
                    <option>Cordoba</option>
                    <option>Rio Cuarto</option>
                    <option>Villa Maria</option>
                  </select>
                </div>
              </div>
            </div>
          </div>

          <!-- Destination city -->
          <div class="row">
            <div class="col-md-6 col-md-offset-1">
              <div class="form-group">
                <div class="col-md-6">
                  <h3>Ciudad de Destino</h3>
                  <select class="form-control" id="destiny">
                    <option></option>
                    <option>Cordoba</option>
                    <option>Rio Cuarto</option>
                    <option>Villa Maria</option>
                  </select>
                </div>
              </div>
            </div>
          </div>

          <!-- Seating selection -->
          <div class="row">
            <div class="col-md-6 col-md-offset-1">
              <div class="form-group">
                <div class="col-md-6">
                   <h3>Cantidad de asientos</h3>
                  <select class="form-control" id="seat">
                    <option></option>
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                  </select>
                </div>
              </div>
            </div>
          </div>
          
          <!-- buton search -->
          <div class="row">
            <div class="col-md-12">
              <button onclick="list_options()" class="btn btn-lg btn-primary">Busqueda</button>
            </div>
          </div>
          <br></br>
          <%- include travels.html.ejs %>
        </div>
      </div>
    </div>
  </body>

  <footer class="jumbotron">
    <font color=#black><center>&copy;monticas.com</center></font>
  </footer> 
</html>
